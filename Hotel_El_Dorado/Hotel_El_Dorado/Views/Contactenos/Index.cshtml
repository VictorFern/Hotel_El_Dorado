@{ ViewData["Title"] = "Contactenos";
                Layout = "_Layout"; }

<div class="contactenosTexto">
    <h2>Contactenos</h2>
</div>
@{ string[,] data = (string[,])ViewData["data"]; }


<div class="row">
    @for (int i = 0; i < data.GetLength(0); i++)
    {
  <div class="col-md-8 justify-content-center">
      <div class="contactenosTexto">
          <p>@data[i,0]</p>
      </div>
      <br />
  </div>
    <br />           
    }
</div>
